put, loss= 'LeastSquares')\n","\n","  layer_hess = list(hess.values())\n","  minimum_ratio = []\n","\n","  for h in layer_hess:\n","    size = h.shape[0] * h.shape[1]\n","    h = h.reshpae(size, size)\n","    h_eig = torch.symeig(h).eigenvalues\n","    num_greater = torch.sum(h_eig > 0).item()\n","    minimum_ratio.append(num_greater / len(h_eig))\n","\n","  ratio_mean = np.mean(minimum_ratio)\n","  return ratio_mean"],"metadata":{"id":"0fyJbgXXXvM_"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["def main(model, train, target):\n","  criterion = nn.MSELoss()\n","  gradient_norm = compute_gradient_norm(model, criterion, train, target)\n","  minimum_ratio = compute_minimum_ratio(model, criterion, train, target)\n","\n","  print('gradient norm: {}, minimum ratio: {}'.format(gradient_norm, minimum_ratio))"],"metadata":{"id":"TboinVOHZnTA"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["if __name__ == '__main__':\n","  torch.manual_seed(0)\n","\n","  activations = defaultdict(int)\n","  hess = defaultdict(float)\n","\n","  main(model, train, target)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":781},"id":"BGK9RQUyaQRY","executionInfo":{"status":"error","timestamp":1642647762412,"user_tz":-480,"elapsed":294,"user":{"displayName":"mark wu","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"04816660482965861860"}},"outputId":"723c0f6c-079b-4f52-e1c4-e67c4f3c47c0"},"execution_count":null,"outputs":[{"output_type":"error","ename":"NotImplementedError","evalue":"ignored","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mNotImplementedError\u001b[0m                       Traceback (most recent call last)","\u001b[0;32m<ipython-input-24-e0dd14c9d552>\u001b[0m in \u001b[0;36m<module>\u001b[0;34m()\u001b[0m\n\u001b[1;32m      5\u001b[0m   \u001b[0mhess\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0mdefaultdict\u001b[0m\u001